<h1>New pcourse</h1>

<%= form_for(@pcourse) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :course_id %><br />
    <%= f.text_field :course_id %>
  </p>
  <p>
    <%= f.label :programme_id %><br />
    <%= f.text_field :programme_id %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', pcourses_path %>
