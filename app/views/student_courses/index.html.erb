<h1>Listing student_courses</h1>

<table>
  <tr>
    <th>Student</th>
    <th>Course</th>
    <th>Term</th>
    <th>Year</th>
    <th>Grade</th>
  </tr>

<% for student_course in @student_courses %>
  <tr>
    <td><%=h student_course.student_id %></td>
    <td><%=h student_course.course_id %></td>
    <td><%=h student_course.term %></td>
    <td><%=h student_course.year %></td>
    <td><%=h student_course.grade %></td>
    <td><%= link_to 'Show', student_course %></td>
    <td><%= link_to 'Edit', edit_student_course_path(student_course) %></td>
    <td><%= link_to 'Destroy', student_course, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New student_course', new_student_course_path %>
